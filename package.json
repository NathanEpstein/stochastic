{
  "name": "@defrecord/stochastic",
  "version": "0.1.1",
  "description": "Simple functions for simulating Poisson Processes, Brownian motion, geometric Brownian motion, discrete/continuous-time Markov chains, and normal random variables",
  "main": "dist/index.js",
  "keywords": [
    "stochastic",
    "histogram",
    "Markov",
    "probability",
    "Brownian",
    "Normal",
    "Poisson"
  ],
  "author": {
    "name": "Jason Walsh",
    "email": "j@wal.sh"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel src/index.js > dist/index.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "./node_modules/.bin/eslint index.js",
    "examples": "for A in examples/*.js ; do ./node_modules/.bin/babel-node $A; done",
    "start": "./node_modules/.bin/babel-node index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwalsh/stochastic.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "eslint": "^3.5.0",
    "flow-bin": "^0.32.0",
    "plotter": "^0.5.0"
  }
}
