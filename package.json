{
  "name": "@jwalsh/stochastic",
  "version": "0.1.3",
  "description": "Simple functions for simulating Poisson Processes, Brownian motion, geometric Brownian motion, discrete/continuous-time Markov chains, and normal random variables",
  "main": "dist/index.js",
  "keywords": [
    "stochastic",
    "histogram",
    "Markov",
    "probability",
    "Brownian",
    "Normal",
    "Poisson"
  ],
  "author": {
    "name": "Jason Walsh",
    "email": "j@wal.sh"
  },
  "scripts": {
      "docs": "./node_modules/.bin/documentation readme -s API src/index.js",
      "examples": "for A in examples/*.js ; do ./node_modules/.bin/babel-node $A; done",
      "flow": "flow; test $? -eq 0 -o $? -eq 2",
      "lint": "./node_modules/.bin/eslint src",
      "prepublish": "./node_modules/.bin/babel src --out-dir dist",
      "release": "npm version patch && git push && git push origin --tags && npm publish --access public",
      "start": "./node_modules/.bin/babel-node src/index.js",
      "test": "./node_modules/.bin/babel-node tests/index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwalsh/stochastic.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "documentation": "^4.0.0-beta10",
    "eslint": "^3.5.0",
    "flow-bin": "^0.32.0",
    "plotter": "^0.5.0"
  }
}
